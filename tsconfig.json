{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@ui/*": [
        "src/ui/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
