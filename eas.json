{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": false
      },
      "ios": {
        "resourceClass": "m-medium",
        "cache": {
          "disabled": false
        }
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "cache": {
          "disabled": false
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": false
      },
      "ios": {
        "simulator": false,
        "resourceClass": "m-medium",
        "cache": {
          "disabled": false
        }
      },
      "android": {
        "buildType": "apk",
        "cache": {
          "disabled": false
        }
      },
      "channel": "preview"
    },
    "production": {
      "cache": {
        "disabled": false
      },
      "ios": {
        "resourceClass": "m-medium",
        "cache": {
          "disabled": false
        }
      },
      "android": {
        "buildType": "app-bundle",
        "cache": {
          "disabled": false
        }
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
